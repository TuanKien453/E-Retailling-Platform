@{
    ViewData["Title"] = "RegisterSucceed";
    Layout = null;
}
<link rel="stylesheet" href="/kit/css/material-kit.min.css" />
<div class="page-header align-items-start min-vh-100" style="background-image: url('https://images.unsplash.com/uploads/1413399939678471ea070/2c0343f7?ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&ixlib=rb-1.2.1&auto=format&fit=crop&w=1950&q=80');" loading="lazy">
    <span class="mask bg-gradient-dark opacity-6"></span>
    <div class="container my-auto">
        <div class="row">
            <div class="col-lg-12 m-auto text-center">
                <h2 class="text-white">Thank you for Sign Up</h2>
                <p class="lead text-white">Please Sign in to see our website.</p>
                @if (ViewBag.Id == 1)
                {
                    <form asp-controller="Login" asp-action="Index">
                        <button type="submit" class="btn bg-gradient-warning mt-4">Go to Sign In</button>
                    </form>
                    
                } else
                {
                    <form asp-controller="SellerLogin" asp-action="Index">
                        <button type="submit" class="btn bg-gradient-warning mt-4">Go to Sign In</button>
                    </form>
                }
                
            </div>
        </div>
    </div>
</div>